<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.1//EN"
                         "http://java.sun.com/dtd/slee-sbb-jar_1_1.dtd">
<sbb-jar>
	<sbb id="ra-bounce-sbb">
		<description>Bounce SBB for training example2</description>

		<sbb-name>BounceSbbTrainingExample2</sbb-name>
		<sbb-vendor>org.mobicents</sbb-vendor>
		<sbb-version>2.0</sbb-version>

		<sbb-ref>
			<sbb-name>ChildSbbTrainingExample2</sbb-name>
			<sbb-vendor>org.mobicents</sbb-vendor>
			<sbb-version>2.0</sbb-version>
			<sbb-alias>ChildSbbTrainingExample2</sbb-alias>
		</sbb-ref>

		<sbb-classes>
			<sbb-abstract-class>
				<sbb-abstract-class-name>
					org.mobicents.slee.training.example2.BounceSbb
				</sbb-abstract-class-name>
				<cmp-field>
					<cmp-field-name>childSbbLocalObject</cmp-field-name>
				</cmp-field>
				<get-child-relation-method>
					<sbb-alias-ref>
						ChildSbbTrainingExample2
					</sbb-alias-ref>
					<get-child-relation-method-name>
						getChildSbbTrainingExample2
					</get-child-relation-method-name>
					<default-priority>0</default-priority>
				</get-child-relation-method>

			</sbb-abstract-class>
		</sbb-classes>

		<event event-direction="Receive" initial-event="True">
			<event-name>InitEvent</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.slee.resource.lab.message.incoming.INIT
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>2.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
			<!-- <initial-event-selector-method-name>callIDSelect</initial-event-selector-method-name> -->
		</event>

		<event event-direction="Receive" initial-event="False">
			<event-name>EndEvent</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.slee.resource.lab.message.incoming.END
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>2.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>


		<resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>
					LabResourceAdaptorType
				</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>
					org.mobicents
				</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>
					2.0
				</resource-adaptor-type-version>
			</resource-adaptor-type-ref>

			<activity-context-interface-factory-name>
				slee/resources/LabRA/raframeacif
			</activity-context-interface-factory-name>

			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					slee/resources/raframe/2.0/sbb2ra
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					LabRA
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>

		</resource-adaptor-type-binding>

	</sbb>


	<sbb id="ra-bounce-child-sbb">
		<description>Child SBB for training example2</description>

		<sbb-name>ChildSbbTrainingExample2</sbb-name>
		<sbb-vendor>org.mobicents</sbb-vendor>
		<sbb-version>2.0</sbb-version>
		<sbb-alias>ChildSbbTrainingExample2</sbb-alias>

		<sbb-classes>
			<sbb-abstract-class>
				<sbb-abstract-class-name>
					org.mobicents.slee.training.example2.ChildSbb
				</sbb-abstract-class-name>
				<cmp-field>
					<cmp-field-name>noOfAnyMessages</cmp-field-name>
				</cmp-field>
			</sbb-abstract-class>
			<sbb-local-interface>
				<sbb-local-interface-name>
					org.mobicents.slee.training.example2.ChildSbbLocalObject
				</sbb-local-interface-name>
			</sbb-local-interface>

		</sbb-classes>

		<event event-direction="Receive" initial-event="False">
			<event-name>AnyEvent</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.slee.resource.lab.message.incoming.ANY
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>2.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>
		<env-entry>
			<description>
				Set the custom response message here
			</description>
			<env-entry-name>customMessage</env-entry-name>
			<env-entry-type>java.lang.String</env-entry-type>
			<env-entry-value>
				What you want to have here:
			</env-entry-value>
		</env-entry>
		<!-- JSLEE v1.1 Specification, Early Draft Review Page 26 -->
		<resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>
					LabResourceAdaptorType
				</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>
					org.mobicents
				</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>
					2.0
				</resource-adaptor-type-version>
			</resource-adaptor-type-ref>

			<activity-context-interface-factory-name>
				slee/resources/LabRA/raframeacif
			</activity-context-interface-factory-name>

			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					slee/resources/raframe/2.0/sbb2ra
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					LabRA
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>

		</resource-adaptor-type-binding>

	</sbb>



</sbb-jar>
