<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">	
	
	<modelVersion>4.0.0</modelVersion>
		
	<parent>
		<artifactId>mobicents-slee-example-xcap-client</artifactId>
		<groupId>org.mobicents.examples</groupId>
		<version>2.0.0-SNAPSHOT</version>
	</parent>
	
	<artifactId>mobicents-slee-example-xcap-client-library</artifactId>
		
	<dependencies>
		<dependency>
			<groupId>org.mobicents.servers.xdm</groupId>
			<artifactId>commons</artifactId>
			<version>1.0.0.CR1</version>
		</dependency>
		<dependency>
			<groupId>org.mobicents.servers.xdm</groupId>
			<artifactId>client-appusages-resource-lists</artifactId>
			<version>1.0.0.CR1</version>
		</dependency>
		<dependency>
			<artifactId>rome</artifactId>
			<groupId>rome</groupId>		
		</dependency>
	</dependencies>
		
	<build>
		<plugins>
			<plugin>
				<groupId>org.mobicents.tools</groupId>
				<artifactId>maven-library-plugin</artifactId>
				<executions>
					<execution>
						<goals>
							<goal>copy-dependencies</goal>
							<goal>generate-descriptor</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<library-name>xcap-client-library</library-name>
					<library-vendor>org.mobicents.examples</library-vendor>
					<library-version>1.0</library-version>				
				</configuration>
			</plugin>
		</plugins>
	</build>
	
</project>