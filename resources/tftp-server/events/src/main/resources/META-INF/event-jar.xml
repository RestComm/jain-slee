<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE event-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Event 1.1//EN" "http://java.sun.com/dtd/slee-event-jar_1_1.dtd">
<event-jar>	

    <library-ref>
        <library-name>tftp-library</library-name>
        <library-vendor>org.mobicents</library-vendor>
        <library-version>1.0</library-version>
    </library-ref> 

	<!-- Request events -->
	<event-definition>
		<event-type-name>net.java.slee.resource.tftp.events.incoming.request.READ</event-type-name>
		<event-type-vendor>net.java.slee</event-type-vendor>
		<event-type-version>1.0</event-type-version>
		<event-class-name>net.java.slee.resource.tftp.events.RequestEvent</event-class-name>
	</event-definition>	

	<event-definition>
		<event-type-name>net.java.slee.resource.tftp.events.incoming.request.WRITE</event-type-name>
		<event-type-vendor>net.java.slee</event-type-vendor>
		<event-type-version>1.0</event-type-version>
		<event-class-name>net.java.slee.resource.tftp.events.RequestEvent</event-class-name>
	</event-definition>	

    <event-definition>
        <event-type-name>net.java.slee.resource.tftp.events.incoming.request.DATA</event-type-name>
        <event-type-vendor>net.java.slee</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>net.java.slee.resource.tftp.events.RequestEvent</event-class-name>
    </event-definition> 

    <event-definition>
        <event-type-name>net.java.slee.resource.tftp.events.incoming.request.ACK</event-type-name>
        <event-type-vendor>net.java.slee</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>net.java.slee.resource.tftp.events.RequestEvent</event-class-name>
    </event-definition> 

    <event-definition>
        <event-type-name>net.java.slee.resource.tftp.events.incoming.request.ERROR</event-type-name>
        <event-type-vendor>net.java.slee</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>net.java.slee.resource.tftp.events.RequestEvent</event-class-name>
    </event-definition> 

</event-jar>
