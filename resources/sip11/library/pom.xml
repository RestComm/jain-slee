<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">	
	
	<modelVersion>4.0.0</modelVersion>
	
	<packaging>jainslee-library</packaging>
	
	<parent>
		<artifactId>sip11-parent</artifactId>
		<groupId>org.mobicents.resources</groupId>
		<version>2.0.0.BETA1</version>
	</parent>
	
	<artifactId>sip11-library</artifactId>
	
	<name>Mobicents :: Resources :: ${pom.artifactId}</name>
	
	<dependencies>
		<dependency>
			<groupId>javax.sip</groupId>
			<artifactId>jain-sip-ri</artifactId>		
		</dependency>
		<dependency>
			<groupId>javax.sip</groupId>
			<artifactId>jain-sip-api</artifactId>
		</dependency>
	</dependencies>
	
	<build>
		<plugins>
			<plugin>
				<groupId>org.mobicents.tools</groupId>
				<artifactId>maven-library-plugin</artifactId>
				<version>${mobicents.tools.mavenplugin.library.version}</version>
				<extensions>true</extensions>
				<configuration>
					<library-name>jain-sip</library-name>
					<library-vendor>javax.sip</library-vendor>
					<library-version>1.2</library-version>
				</configuration>
			</plugin>
		</plugins>
	</build>
	
</project>